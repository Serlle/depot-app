<%= turbo_stream.replace 'cart' do %>
  <%= render partial: 'layouts/cart', locals: { cart: @cart }%>
<% end %>

<%= turbo_stream.replace 'product' do %>
  <%= render partial: 'store/product', collection: @products %>
<% end %>

<%= turbo_stream.replace 'visit' do %>
  <%= render partial: 'layouts/visit', locals: { visit: @visit }%>
<% end %>